cat > /home/victorcostac/test-api.sh << 'EOF'
#!/bin/bash
echo "=========================================="
echo "ðŸŽ¬ TESTANDO API PLOCADORA"
echo "=========================================="
echo ""
BASE_URL="http://localhost:8080"
# Cores para output
GREEN='\033[0;32m'
RED='\033[0;31m'
YELLOW='\033[1;33m'
NC='\033[0m'
# VariÃ¡veis para IDs
DIRETOR_ID=""
CLASSE_ID=""
ATOR_ID=""
TITULO_ID=""
ITEM_ID=""
echo "=========================================="
echo "ðŸ“‹ TESTE 1: CRUD de DIRETOR"
echo "=========================================="
# POST - Criar Diretor
echo "1.1) Criar Diretor"
response=$(curl -s -w "\n%{http_code}" -X POST "$BASE_URL/diretor" \
    -H "Content-Type: application/json" \
    -d '{"nome": "Steven Spielberg"}')
http_code=$(echo "$response" | tail -n1)
body=$(echo "$response" | head -n-1)
if [ "$http_code" == "201" ]; then
    echo -e "${GREEN}âœ“ Diretor criado (Status: $http_code)${NC}"
    DIRETOR_ID=$(echo "$body" | grep -o '"id":[0-9]*' | grep -o '[0-9]*')
    echo "  ID: $DIRETOR_ID - Response: $body"
else
    echo -e "${RED}âœ— Falha (Status: $http_code) - $body${NC}"
fi
echo ""
# GET - Listar diretores
echo "1.2) Listar diretores"
response=$(curl -s -w "\n%{http_code}" -X GET "$BASE_URL/diretor")
http_code=$(echo "$response" | tail -n1)
if [ "$http_code" == "200" ]; then
    echo -e "${GREEN}âœ“ Listagem OK (Status: $http_code)${NC}"
else
    echo -e "${RED}âœ— Falha (Status: $http_code)${NC}"
fi
echo ""
# GET - Buscar por ID
if [ ! -z "$DIRETOR_ID" ]; then
    echo "1.3) Buscar diretor por ID"
    response=$(curl -s -w "\n%{http_code}" -X GET "$BASE_URL/diretor/$DIRETOR_ID")
    http_code=$(echo "$response" | tail -n1)
    body=$(echo "$response" | head -n-1)
    if [ "$http_code" == "200" ]; then
        echo -e "${GREEN}âœ“ Busca OK - $body${NC}"
    else
        echo -e "${RED}âœ— Falha (Status: $http_code)${NC}"
    fi
    echo ""
fi
# PUT - Atualizar
if [ ! -z "$DIRETOR_ID" ]; then
    echo "1.4) Atualizar diretor"
    response=$(curl -s -w "\n%{http_code}" -X PUT "$BASE_URL/diretor/$DIRETOR_ID" \
        -H "Content-Type: application/json" \
        -d "{\"id\": $DIRETOR_ID, \"nome\": \"Steven Spielberg Updated\"}")
    http_code=$(echo "$response" | tail -n1)
    if [ "$http_code" == "200" ]; then
        echo -e "${GREEN}âœ“ AtualizaÃ§Ã£o OK${NC}"
    else
        echo -e "${RED}âœ— Falha (Status: $http_code)${NC}"
    fi
    echo ""
fi
echo "=========================================="
echo "ðŸŽ­ TESTE 2: CRUD de ATOR"
echo "=========================================="
# POST - Criar Ator
echo "2.1) Criar Ator"
response=$(curl -s -w "\n%{http_code}" -X POST "$BASE_URL/ator" \
    -H "Content-Type: application/json" \
    -d '{"nome": "Tom Hanks"}')
http_code=$(echo "$response" | tail -n1)
body=$(echo "$response" | head -n-1)
if [ "$http_code" == "201" ]; then
    echo -e "${GREEN}âœ“ Ator criado${NC}"
    ATOR_ID=$(echo "$body" | grep -o '"id":[0-9]*' | grep -o '[0-9]*')
    echo "  ID: $ATOR_ID - $body"
else
    echo -e "${RED}âœ— Falha (Status: $http_code)${NC}"
fi
echo ""
# GET - Listar atores
echo "2.2) Listar atores"
response=$(curl -s -w "\n%{http_code}" -X GET "$BASE_URL/ator")
http_code=$(echo "$response" | tail -n1)
if [ "$http_code" == "200" ]; then
    echo -e "${GREEN}âœ“ Listagem OK${NC}"
else
    echo -e "${RED}âœ— Falha${NC}"
fi
echo ""
echo "=========================================="
echo "ðŸ’° TESTE 3: CRUD de CLASSE"
echo "=========================================="
# POST - Criar Classe
echo "3.1) Criar Classe"
response=$(curl -s -w "\n%{http_code}" -X POST "$BASE_URL/classe" \
    -H "Content-Type: application/json" \
    -d '{"nome": "LanÃ§amento", "valor": 10.50, "prazo_devolucao": "3"}')
http_code=$(echo "$response" | tail -n1)
body=$(echo "$response" | head -n-1)
if [ "$http_code" == "201" ]; then
    echo -e "${GREEN}âœ“ Classe criada${NC}"
    CLASSE_ID=$(echo "$body" | grep -o '"id":[0-9]*' | grep -o '[0-9]*')
    echo "  ID: $CLASSE_ID - $body"
else
    echo -e "${RED}âœ— Falha (Status: $http_code) - $body${NC}"
fi
echo ""
echo "=========================================="
echo "ðŸŽ¬ TESTE 4: CRUD de TITULO"
echo "=========================================="
if [ ! -z "$DIRETOR_ID" ] && [ ! -z "$CLASSE_ID" ] && [ ! -z "$ATOR_ID" ]; then
    echo "4.1) Criar TÃ­tulo"
    response=$(curl -s -w "\n%{http_code}" -X POST "$BASE_URL/titulo" \
        -H "Content-Type: application/json" \
        -d "{
            \"nome\": \"Forrest Gump\",
            \"ano\": 1994,
            \"sinopse\": \"A histÃ³ria de um homem simples\",
            \"categoria\": \"Drama\",
            \"id_classe\": $CLASSE_ID,
            \"id_diretor\": $DIRETOR_ID,
            \"id_ator\": $ATOR_ID
        }")
    http_code=$(echo "$response" | tail -n1)
    body=$(echo "$response" | head -n-1)
    if [ "$http_code" == "201" ]; then
        echo -e "${GREEN}âœ“ TÃ­tulo criado${NC}"
        TITULO_ID=$(echo "$body" | grep -o '"id":[0-9]*' | head -1 | grep -o '[0-9]*')
        echo "  ID: $TITULO_ID"
        echo "  Response: $(echo $body | head -c 200)..."
    else
        echo -e "${RED}âœ— Falha (Status: $http_code)${NC}"
        echo "  $body"
    fi
    echo ""
    # GET - Listar tÃ­tulos
    echo "4.2) Listar tÃ­tulos (com JOIN FETCH)"
    response=$(curl -s -w "\n%{http_code}" -X GET "$BASE_URL/titulo")
    http_code=$(echo "$response" | tail -n1)
    body=$(echo "$response" | head -n-1)
    if [ "$http_code" == "200" ]; then
        echo -e "${GREEN}âœ“ Listagem OK - verificando relacionamentos...${NC}"
        echo "$body" | grep -q "diretor" && echo "  âœ“ Diretor carregado"
        echo "$body" | grep -q "classe" && echo "  âœ“ Classe carregada"
        echo "$body" | grep -q "atores" && echo "  âœ“ Atores carregados"
    else
        echo -e "${RED}âœ— Falha${NC}"
    fi
    echo ""
fi
echo "=========================================="
echo "ðŸ’¿ TESTE 5: CRUD de ITEM"
echo "=========================================="
if [ ! -z "$TITULO_ID" ]; then
    echo "5.1) Criar Item"
    response=$(curl -s -w "\n%{http_code}" -X POST "$BASE_URL/item" \
        -H "Content-Type: application/json" \
        -d "{
            \"num_serie\": 12345,
            \"dt_aquisicao\": \"2024-01-15\",
            \"tipo_item\": \"DVD\",
            \"titulo_id\": $TITULO_ID
        }")
    http_code=$(echo "$response" | tail -n1)
    body=$(echo "$response" | head -n-1)
    if [ "$http_code" == "201" ]; then
        echo -e "${GREEN}âœ“ Item criado${NC}"
        ITEM_ID=$(echo "$body" | grep -o '"id":[0-9]*' | head -1 | grep -o '[0-9]*')
        echo "  ID: $ITEM_ID"
    else
        echo -e "${RED}âœ— Falha (Status: $http_code) - $body${NC}"
    fi
    echo ""
    # GET - Buscar item com relacionamentos
    if [ ! -z "$ITEM_ID" ]; then
        echo "5.2) Buscar item (com JOIN FETCH do tÃ­tulo)"
        response=$(curl -s -w "\n%{http_code}" -X GET "$BASE_URL/item/$ITEM_ID")
        http_code=$(echo "$response" | tail -n1)
        body=$(echo "$response" | head -n-1)
        if [ "$http_code" == "200" ]; then
            echo -e "${GREEN}âœ“ Busca OK - verificando relacionamentos...${NC}"
            echo "$body" | grep -q "titulo" && echo "  âœ“ TÃ­tulo carregado"
        else
            echo -e "${RED}âœ— Falha${NC}"
        fi
        echo ""
    fi
fi
echo "=========================================="
echo "âŒ TESTE 6: CASOS DE ERRO"
echo "=========================================="
echo "6.1) Buscar ator inexistente"
response=$(curl -s -w "\n%{http_code}" -X GET "$BASE_URL/ator/99999")
http_code=$(echo "$response" | tail -n1)
if [ "$http_code" == "404" ]; then
    echo -e "${GREEN}âœ“ 404 retornado corretamente${NC}"
else
    echo -e "${RED}âœ— Esperado 404, recebido $http_code${NC}"
fi
echo ""
echo "6.2) Criar ator sem nome"
response=$(curl -s -w "\n%{http_code}" -X POST "$BASE_URL/ator" \
    -H "Content-Type: application/json" \
    -d '{}')
http_code=$(echo "$response" | tail -n1)
if [ "$http_code" == "400" ]; then
    echo -e "${GREEN}âœ“ 400 Bad Request retornado corretamente${NC}"
else
    echo -e "${RED}âœ— Esperado 400, recebido $http_code${NC}"
fi
echo ""
echo "=========================================="
echo "ðŸ—‘ï¸  TESTE 7: DELEÃ‡Ã•ES (na ordem correta)"
echo "=========================================="
[ ! -z "$ITEM_ID" ] && curl -s -X DELETE "$BASE_URL/item/$ITEM_ID" && echo "âœ“ Item deletado"
[ ! -z "$TITULO_ID" ] && curl -s -X DELETE "$BASE_URL/titulo/$TITULO_ID" && echo "âœ“ TÃ­tulo deletado"
[ ! -z "$ATOR_ID" ] && curl -s -X DELETE "$BASE_URL/ator/$ATOR_ID" && echo "âœ“ Ator deletado"
[ ! -z "$CLASSE_ID" ] && curl -s -X DELETE "$BASE_URL/classe/$CLASSE_ID" && echo "âœ“ Classe deletada"
[ ! -z "$DIRETOR_ID" ] && curl -s -X DELETE "$BASE_URL/diretor/$DIRETOR_ID" && echo "âœ“ Diretor deletado"
echo ""
echo "=========================================="
echo "âœ… TESTES CONCLUÃDOS!"
echo "=========================================="
EOF

